.add-form select.sub-select, .add-form select.sub-status, .add-form .sub-date{
	width:27% !important;
	font-size:1em;
	color:#000;
	margin-left:2% !important;
	margin-right:0% !important;
}
.add-form .goal-type, .add-form .goal-status{
	position:relative;
	width:32%;
}
.add-form #date-picker{
	margin-bottom:1em;
}
.add-form input[type="text"].sub-input{
	width:90%;
	margin-left:6%;
	margin-bottom:0.5em;
}
.goal-top{
	position:relative;
	float:left;
	width:100%;
}
.goal-top span, .goal-top a{
	position:relative;
	float:left;
	margin-right:2%;
	margin-bottom:0.5em;
	font-size:1.2em;
	color:#535353;
}
.goal-top a{
	color:#999999;
	font-size:1em;
	line-height:1.5em;
}
.goal-top a:hover{
	color:#696969;
}
#sub-goal{
	position:relative;
	float:right;
	width:100%;
	text-align:right;
}
.new-sub-goal {
	position:relative;
	float:right !important;
  	height: 1.4em !important;
	padding-top:0.2em !important;
	margin-right:0 !important;
	margin-left:0.2em;
  	border-radius: 0.8em;
  	color: #fff;
  	text-align: center;
	text-decoration:none;
}
.new-sub-goal:hover{
	color: #000000;
}
.remove-goal{
	margin-right:0.5em !important;
}
.sub-container{
	position:relative;
	float:right;
	width:96%;
	display:block;
	padding-top:0.5em;
	font-size:1em;

}

.sub-container h4{
	text-align:left;
}


.add-form h4 a.moveup,.add-form a.movedown{
	display:inherit !important;
	width:inherit !important;
	margin-right:5px;
	text-decoration:none;
	margin-top:0px;
	font-size:0.8em;
}
input[disabled], select[disabled], textarea[disabled], input[readonly], select[readonly], textarea[readonly] {
	cursor: pointer !important;
}
.goal-top input.goal-due, .goal-top input.habit-start-date {
	display:none;
	float:left;
	font-weight:bold;
	font-size:1em;
	color:#999999;
	background: rgb(0, 0, 0) transparent;
	background: rgba(0, 0, 0, 0);
	/* IE 6/7 */
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#00000000, endColorstr=#00000000);
	/* IE8 */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#00000000, endColorstr=#00000000)";
	zoom: 1;
	border:none;
	width:auto;
	padding:0;
	-webkit-box-shadow: none;
	-moz-box-shadow: none;
	box-shadow: none;
	-webkit-transition: none;
	-moz-transition: none;
	-o-transition: none;
	transition: none;
}
input.goal-due:hover, input.habit-start-date:hover {
	cursor:pointer !important;
	color:#696969;
}
/*POP UP DIV*/
.modal{
	position: fixed;
  	z-index: 1500;
  	background-color: #ffffff;
  	border: 1px solid #999;
  	border: 1px solid rgba(0, 0, 0, 0.3);
