body{
	background: #1d1f20;
	color:#fff;
	font-family:Helvetica, Arial, sans-serif;
	font-size:0.85em;
}
.no-bg{
	background: none !important;
}
.red{
	color:#ff0000 !important;
}
.yellow{
	color:#fec10f;
}
.green{
	color:#00BD30;
}
#home{
    position: absolute;
    top:0;
    bottom:0;
    right:0;
    left:0;
	overflow:hidden;
}
ul, li{
	list-style:none;
	margin:0;
	padding:0;
}
a, a:hover{
	color:#fff;
	text-decoration:none;
}
/*HEADER*/
.main-header{
	display:block;
	position:relative;
	z-index:200;
	background:#212121;
	height:5em;
	border-bottom:1px solid black;
}
.group:before,.group:after{
	content:"";
	display:table;
}
.group:after{
	clear:both;
}
.logo{
	float:left;
	margin:0;
	position:relative;
	z-index:1;
	width:5.5em;
	padding:0em;
	border-bottom:1px solid black;
	background:url(data:image/svg+xml;
	base64,);
	background:-webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #4d4d4d), color-stop(100%, #2f2f2f));
	background:-webkit-linear-gradient(#4d4d4d,#2f2f2f);
	background:-moz-linear-gradient(#4d4d4d,#2f2f2f);
	background:-o-linear-gradient(#4d4d4d,#2f2f2f);
	background:linear-gradient(#4d4d4d,#2f2f2f);
	border-radius:0.15em;
	margin-left:0.3em;
	height:1.1em;
	margin-top:0.4em;
 }
.logo:hover{
	background:#333 !important;
	text-decoration:none !important;
	-webkit-box-shadow:0 2px 3px -2px rgba(0,0,0,0.5);
	-moz-box-shadow:0 2px 3px -2px rgba(0,0,0,0.5);
	box-shadow:0 2px 3px -2px rgba(0,0,0,0.5);
}
.logo a{
	background:url(../img/logo.png) center center no-repeat;
	background-size:contain;
	width:100%;
	height:95%;
	display:block;
	border:0;
	outline:0;
	text-indent:-999px;
	padding:0;
}
.login{
	width:100% !important;
	height: 100%;
	float: left;
	margin: 0;
	position: relative;
	z-index: 1;
	width: 6.5em;
	padding: 0% 0.2em;
	border-bottom: 1px solid black;
	background: url(data:image/svg+xml; base64,PD94bWwgdmVyc2lvbj0iMS4wIiBlbmNvZGluZz0idXRmLTgâ€¦dpZHRoPSIxMDAlIiBoZWlnaHQ9IjEwMCUiIGZpbGw9InVybCgjZ3JhZCkiIC8+PC9zdmc+IA==);
	background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #4d4d4d), color-stop(100%, #2f2f2f));
	background: -webkit-linear-gradient(#4d4d4d,#2f2f2f);
	background: -moz-linear-gradient(#4d4d4d,#2f2f2f);
	background: -o-linear-gradient(#4d4d4d,#2f2f2f);
	background: linear-gradient(#4d4d4d,#2f2f2f);
}
.login a{
	background:url(../img/logo.png) center center no-repeat;
	background-size:contain;
}
.main-header ul.nav{
	position:relative;
	list-style:none;
	margin-top:1em;
}
.main-header ul.nav li:first-child{
	margin-left:1em;
}
.button{
	display:inline-block;
	border:0;
	outline:0;
	padding:0.8em 1em;
	line-height:1em;
	background:url(data:image/svg+xml;
	base64,);
	background:-webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #4d4d4d), color-stop(100%, #2f2f2f));
	background:-webkit-linear-gradient(#4d4d4d,#2f2f2f);
	background:-moz-linear-gradient(#4d4d4d,#2f2f2f);
	background:-o-linear-gradient(#4d4d4d,#2f2f2f);
	background:linear-gradient(#4d4d4d,#2f2f2f);
	border-radius:0.3em;
	border:1px solid #111;
	color:white !important;
	font-size:1.2em;
	white-space:nowrap;
	text-overflow:ellipsis;k
	cursor:pointer;
}

.button:hover, .button-active{
	background:#333 !important;
	text-decoration:none !important;
	-webkit-box-shadow:0 2px 3px -2px rgba(0,0,0,0.5);
	-moz-box-shadow:0 2px 3px -2px rgba(0,0,0,0.5);
	box-shadow:0 2px 3px -2px rgba(0,0,0,0.5);
}
.nav .pull-right{
	margin-right:10px;
}
/*HABIT*/
.habit{
	border-top:1px solid #3d3d3d;
	background:#212121;
	width:100%;
	height:40%;
	overflow-y: scroll;
	overflow-x: hidden;
	padding-bottom:1%;
	direction:ltr;
	text-align:left;
}
.habit::-webkit-scrollbar, .task::-webkit-scrollbar, .character::-webkit-scrollbar{
	width:10px;
	height:10px;
}
.habit::-webkit-scrollbar-thumb:vertical, .task::-webkit-scrollbar-thumb:vertical,.character::-webkit-scrollbar-thumb:vertical {
  	height: 20px;
	background: -webkit-gradient(linear, left top, right top, color-stop(0%, #4d4d4d), color-stop(100%, #333333));
  	border: 1px solid #0d0d0d;
  	border-top: 1px solid #666666;
  	border-left: 1px solid #666666;
 }
.habit::-webkit-scrollbar-track,.task::-webkit-scrollbar-track, .character::-webkit-scrollbar-track{
	background:none;
}
.habit::-webkit-scrollbar-thumb,.task::-webkit-scrollbar-thumb,.character::-webkit-scrollbar-thumb{
	border:8px solid #fec10f;
	height:10px;
}

.habit, .habit-title, ul.week-title li{
	background: #1d1f20;
	background: url(data:image/svg+xml;base64,);
	background: -webkit-linear-gradient(45deg, #1d1f20,#2f3031) !important;
	background: -moz-linear-gradient(45deg, #1d1f20,#2f3031) !important;
	background: -o-linear-gradient(45deg, #1d1f20,#2f3031) !important;
	background: linear-gradient(45deg, #1d1f20,#2f3031) !important;
}
.habit-title{
	position:relative;
	display:block;
	padding:0.5em 0em 0.5em 0.8em;
	font-size:1.5em;
}
.habit-title-name{
	color:#fec10f;
}
ul.week-title{
	margin-left:0px;
}
ul.week-title li{
	position:relative;
	float:left;
	padding:0.5em 1em;
	margin-top:-0.7em;
	border-right:solid 1px #444;
	text-align:center;
	font-size:0.8em;
	line-height:1.6em;
	width:2em;
	overflow:hidden;
}

ul.week-title li.current{
	background:#1a1a1a !important;
}

.habit-title:after {
	content: "";
	position: absolute;
	top: 100%;
	left: 0;
	height: 5px;
	width: 100%;
	background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, rgba(0,0,0,0.2)), color-stop(100%, rgba(0,0,0,0)));
	background: -webkit-linear-gradient(rgba(0,0,0,0.2),rgba(0,0,0,0));
	background: -moz-linear-gradient(rgba(0,0,0,0.2),rgba(0,0,0,0));
	background: -o-linear-gradient(rgba(0,0,0,0.2),rgba(0,0,0,0));
	background: linear-gradient(rgba(0,0,0,0.2),rgba(0,0,0,0));
}
ul.habit-list, ul.task-list,  ul.character-list, ul.sub-task-list{
	position:relative;
	float:left;
	width:100%;
	padding:0;
	margin:0;
}
ul.habit-list li, ul.task-list li, ul.sub-task-list li, ul.character-list li{
	position:relative;
	float:left;
	background: #1d1f20;
	background: url(data:image/svg+xml;base64,);
	background: -webkit-linear-gradient(-90deg, #1d1f20,#2f3031) !important;
	background: -moz-linear-gradient(-90deg, #1d1f20,#2f3031) !important;
	background: -o-linear-gradient(-90deg, #1d1f20,#2f3031) !important;
	background: linear-gradient(-90deg, #1d1f20,#2f3031) !important;
	min-height:3em;
	margin:0;
	padding:0;
	margin-bottom:0.4em;
	z-index:0 !important;
}
ul.habit-list li:last-child, ul.character-list li:last-child{
	margin-bottom:1em;
}
.sort-div{
	position:relative;
	float:left;
	padding:1em 0 0 0;
	margin-left:1.5em;
	width:4.5em;
}
ul.task-list .sort-div{
	width:5.3em;
}
a.moveup, a.movedown, a.calendar,a.like{
	position:relative;
	float:left;
	opacity:0.7;
	margin-right:14px;
	margin-left:0em;
	padding:0;
	width:14px;
	cursor:pointer;
}
a.movedown{
	margin-top:0.3em;
}
a.moveup{
	margin-left:-28px;
	margin-top:-0.8em;
}
a.moveup:hover, a.movedown:hover, a.calendar:hover,  a.like:hover{
	opacity:1;
}
.habit-content-div{
	position:relative;
	float:right;
}
.habit-checkbox-container{
	position:relative;
	float:right;
	overflow:hidden;
}
.habit-checkbox{
	position: relative;
	float: left;
	padding: 1.4em 1em 0.6em;
	border-right: solid 1px #444;
	text-align: center;
	font-size: 0.8em;
	line-height: 1.6em;
	width: 2em;
	height:100%;
	overflow:hidden;
}
.first-checkbox{
	border-left: solid 1px #444;
}
.habit-checkbox input[type="checkbox"]{
	margin-top: 0.5em;
}
.regular-checkbox {
	display: none;
}
.regular-checkbox:hover{
}
.regular-checkbox + label {
	background:url(../img/Checkbox-Empty.png) top left no-repeat;
	background-size:contain;
	padding: 0.6em;
	display: block;
	position: relative;
	float:left;
	margin:0.4em 0 0 0.5em;
	border:none;
}
.past-checkbox + label {
	background:url(../img/Checkbox-Filled.png) top left no-repeat;
	background-size:contain;
	padding: 0.6em;
	display: block;
	position: relative;
	float:left;
	margin:0.4em 0 0 0.5em;
	border:none;
}
.regular-checkbox + label:active, .regular-checkbox:checked + label:active {
	background:url(../img/Checkbox-Checked.png) top left no-repeat;
	background-size:contain;
	padding: 0.6em;
	display: block;
	position: relative;
	float:left;
	margin:0.4em 0 0 0.5em;
	border:none;
}

.regular-checkbox:checked + label {
	background:url(../img/Checkbox-Checked.png) top left no-repeat;
	background-size:contain;
	padding: 0.6em;
	display: block;
	position: relative;
	float:left;
	margin:0.4em 0 0 0.5em;
	border:none;
}
.regular-checkbox:checked + label:after {
	font-size: 0.8em;
	position: absolute;
	top: -0.3em;
	left: 0.1em;
	color: #fff;
}
.current-date{
	background: #1a1a1a;
}
.habit-link, .task-link, .character-link, .goal-link{
	position:relative;
	float:left;
	padding:1.1em 1em 0.4em;
	min-height:2.4em;
}
.habit-link{
	padding-bottom:0em;
}
.habit-link a:hover, .task-link a:hover,  .character-link a:hover{
	text-decoration:underline;
}
/*Divider*/
.divider {
	background: url(data:image/svg+xml;base64,PD94bWwgdmVyc2lvbj0iMS4wIiBlbmNvZGluZz0idXRmLTgiâ€¦pZHRoPSIxMDAlIiBoZWlnaHQ9IjEwMCUiIGZpbGw9InVybCgjZ3JhZCkiIC8+PC9zdmc+IA==);
	background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #404040), color-stop(100%, #121212));
	background: -webkit-linear-gradient(#404040,#121212);
	background: -moz-linear-gradient(#404040,#121212);
	background: -o-linear-gradient(#404040,#121212);
	background: linear-gradient(#404040,#121212);
	border-top: 1px solid black;
	border-bottom: 1px solid black;
	box-shadow: inset 0 1px 0 #4e4e4e,0 2px 2px rgba(0,0,0,0.4);
	position: relative;
	float:left;
	width:100%;
	height: 5px;
	margin-top: 1px;
	clear:both;
}
/*Task*/
.task{
	position:relative;
	float:left;
	margin-top:4px;
	height:50%;
	width:65%;
	border-right:solid 1px #444;
	border-top:solid 1px #444;
	background: #191919;
	overflow-y: scroll;
	overflow-x: hidden;
	direction:ltr;
	text-align:left;
}
ul.task-list{
	padding-bottom:1em;
}
ul.task-list li{
	background:none !important;
	width:100%;
}
ul.sub-task-list li{
	margin-top:5px;
	margin-bottom:0px;
}

ul.sub-task-list{
	margin-left:5%;
	width:95%;
	background:none !important;
}
.task-container{
	position:relative;
	float:left;
	width:100%;
	background:#151515;
}
.sub-task-list .task-container{
	background:#333 !important;
}
.task-content-div{
	position:relative;
	float:left;
	width:60%;
}
a.like{
	width:2em !important;
	height:2em !important;
	text-indent:-999px !important;
	background:url(../img/Thumbs-Active.png);
	background-repeat:no-repeat;
	background-size:100% 100%;
	margin-top:-0.3em;
}

.task-date{
	margin-left:15px;
	font-size:0.9em;
	color:#ddd;
}
.task-progress-bar{
	position:relative;
	float:right;
	width:20%;
	margin-right:5%;
	border:solid 1px #fec10f;
	height:12px;
	margin-top:1.5em;
	background:#121212;
	text-align:left;
}
.meter{
	display: block;
	height: 100%;
	position: relative;
	background: #ffebba;
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #ffebba 0%, #ffd65e 5%, #ffbb00 50%, #ff9b05 92%, #ffd7a8 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffebba), color-stop(5%,#ffd65e), color-stop(50%,#ffbb00), color-stop(92%,#ff9b05), color-stop(100%,#ffd7a8));
	background: -webkit-linear-gradient(top,  #ffebba 0%,#ffd65e 5%,#ffbb00 50%,#ff9b05 92%,#ffd7a8 100%);
	background: -o-linear-gradient(top,  #ffebba 0%,#ffd65e 5%,#ffbb00 50%,#ff9b05 92%,#ffd7a8 100%);
	background: -ms-linear-gradient(top,  #ffebba 0%,#ffd65e 5%,#ffbb00 50%,#ff9b05 92%,#ffd7a8 100%);
	background: linear-gradient(to bottom,  #ffebba 0%,#ffd65e 5%,#ffbb00 50%,#ff9b05 92%,#ffd7a8 100%);
	float:left;
}
.progress-text{
	position:absolute;
	top:-4px;
	left:50%;
	margin-left:-8px;
	font-weight:bold;
}
/*Character*/
.character{
	margin-top:4px;
	position:relative;
	float:left;
	height:50%;
	width:34%;
	border-left:solid 1px #555;
	border-top:solid 1px #555;
	background: #191919;
	margin-left:2px;
	overflow-y: scroll;
	overflow-x: hidden;
	direction:ltr;
	text-align:left;
}
ul.character-list{
	padding-bottom:1em;
}
ul.character-list li{
	width:100%;
}
ul.character-list .sort-div{
	width:2.5em;
}
.character-content-div{
	position:relative;
	float:left;
	width: -moz-calc(100% - 4em);
	width: -webkit-calc(100% - 4em);
	width: -o-calc(100% - 4em);
	width: calc(100% - 4em);
}

/*-----------ADD FORM------------------------*/
.add-form{
	position:relative;
	float:left;
	width:88%;
	margin:1.5em 6% 1em;
}
.add-form h3{
	margin-top:0%;
	margin-bottom:3%;
	font-size:1em;
	color: #ffffff;
	text-decoration:underline;
}
.add-form h4{
	margin-left:0%;
	font-size:1.2em;
	color: #535353;
	margin-top:0.2em;
	font-weight:300;
	clear:both;
}

.add-form input[type="text"], .add-form input[type="password"], .add-form  textarea, .add-form input[type="text"]{
	width:98%;
	padding:0.8em 1%;
	color:#000;
	font-weight:bold;
	border:solid 1px #999;
	font-size:1em;
	border-radius:0.4em;
	background: #ffffff;
}
.add-form textarea{
	font-weight:normal;
}
.add-form input::-webkit-input-placeholder { /* WebKit browsers */
    color: #d2d2d2;
	font-weight:normal;
}
.add-form input:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: #d2d2d2;
	font-weight:normal;	
}
.add-form input::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: #d2d2d2;
	font-weight:normal;	
}
.add-form input:-ms-input-placeholder { /* Internet Explorer 10+ */
    color: #fafafa;
	font-weight:normal;	
}
.add-form  textarea{
	height:6em;
}
.add-form input[type="submit"]{
	background: #849db0;
	color:#fff;
	font-weight:bold;
	font-size:1.2em;
	padding:0em 1.2em;
	height:2.5em;
	border:none;
	cursor:pointer;
	display:block;
	float:right;
	position:fixed;
	bottom:0;
	right:1em;
}
.add-form input[type="submit"].delete-btn{
	position:relative;
	float:right;
	margin-right:-10%;
	margin-top:-0.5em;
}
.add-form input[type="submit"]:hover{
	background:#fec10f;
}

